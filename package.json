{
  "name": "ug-js-sdk",
  "version": "3.0.16",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/ug-js-sdk-bundle.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/ug-core/types/index.js",
      "require": "./dist/ug-core/types/index.js",
      "types": "./dist/ug-core/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "bundle": "webpack",
    "bundle:dev": "webpack --mode=development --devtool=source-map",
    "test": "jest --passWithNoTests",
    "format": "prettier --write \"**/*.{js,ts,json,css,md}\"",
    "prepare": "npm run build && npm run bundle",
    "serve:demo": "npm run bundle:dev && http-server public -o /demo.html"
  },
  "author": "Gal Bracha <gal.bracha@uglabs.io>",
  "license": "MIT",
  "homepage": "https://github.com/uglabs/js-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/uglabs/js-sdk"
  },
  "keywords": [
    "ug labs",
    "ai engine",
    "ai for kids"
  ],
  "dependencies": {
    "@ricky0123/vad-web": "^0.0.23",
    "axios": "^1.8.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "http-server": "^14.1.1",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  },
  "overrides": {
    "glob": "^10.4.5"
  }
}
